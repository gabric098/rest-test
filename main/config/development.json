{
  "app": {
    "apiVersion" : "v1",
    "name": "rest-test"
  },
  "server": {
    "port": 3000,
    "name": "localhost",
    "defaultVersion": "1.0.0",
    "acceptable": [ "application/json" ]
  },
  "database" : {
    "host" : "localhost",
    "database" : "dod_dev",
    "port" : "8889",
    "user" : "root",
    "password" : "root",
    "dialect": "mysql"
  },
  "log4js" : {
    "appenders": [
      {
        "type": "logLevelFilter",
        "level": "debug",
        "appender": {
          "type": "file",
          "filename": "./main/logs/dev.log",
          "layout": {
            "type": "pattern",
            "pattern": "[%d] [%h] [%z] [%c] %p: %m"
          }
        }
      }
    ],
    "replaceConsole": true
  }
}
